{
  "name": "lb-stock",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "type": "module",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "electron .",
    "build:front": "vue-cli-service build",
    "build:backend": "cd server && npm run build",
    "build:electron": "electron-packager . --platform=win32 --arch=x64 --out=dist",
    "start": "electron ."
  },
  "dependencies": {
    "axios": "^1.7.9",
    "core-js": "^3.8.3",
    "express": "^4.21.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "vue": "^3.2.13",
    "vue-router": "^4.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "electron": "^34.1.1",
    "electron-builder": "^25.1.8",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "build": {
    "appId": "com.tuempresa.lb-stock",
    "productName": "LB Stock",
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "extraResources": [
      {
        "from": "server",
        "to": "server",
        "filter": [
          "**/*"
        ]
      }
    ],
    "files": [
      "**/*"
    ]
  }
}
